############################
# Makefile for Icinga
############################

###############################
# Debug
###############################
ENABLE_DEBUG=@ENABLE_DEBUG@

# Compiler flags for use with Valgrind - set when debug is enabled
ifeq ('$(ENABLE_DEBUG)', 'yes')
        CFLAGS=-O0 -g -Wall -DHAVE_CONFIG_H -DNSCORE
endif

clean:
	rm -f *.o
	rm -f *~

distclean: clean
	rm -f Makefile

devclean: distclean

